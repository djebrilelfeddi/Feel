    "publish": "electron-forge publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\""
  },
  "config": {
    "forge": "./config/forge.config.js"
  },
  "postcss": {
    "config": "./config/postcss.config.js"
